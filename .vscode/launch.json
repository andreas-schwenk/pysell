{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "test sell.py",
      "type": "debugpy",
      "request": "launch",
      "program": "sell.py",
      "args": ["-J", "examples/ex2.txt"],
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "test index.html",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5501/web/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Node.js: current file",
      "program": "${file}",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: current file",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
